<AnchorPane prefHeight="100.0" prefWidth="300.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="baseline.LoadFileController">
   <children>
      <Pane prefHeight="20.0" prefWidth="200.0" style="-fx-background-color: #2196f3;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <Pane prefHeight="150.0" prefWidth="10.0" style="-fx-background-color: #2196f3;" AnchorPane.leftAnchor="0.0" />
      <Pane layoutX="195.0" prefHeight="150.0" prefWidth="10.0" style="-fx-background-color: #2196f3;" AnchorPane.rightAnchor="0.0" />
      <Pane layoutY="140.0" prefHeight="10.0" prefWidth="200.0" style="-fx-background-color: #2196f3;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <Pane prefHeight="150.0" prefWidth="300.0" style="-fx-background-color: #e1e1e1;" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="100.0">
         <children>
            <Button fx:id="importButton" layoutX="204.0" layoutY="4.0" mnemonicParsing="false" onAction="#loadFile" style="-fx-border-color: #2196f3;" text="Import">
