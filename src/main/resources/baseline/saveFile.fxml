<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="baseline.SaveFileController">
   <children>
      <Pane prefHeight="20.0" prefWidth="200.0" style="-fx-background-color: #2196f3;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <Pane prefHeight="150.0" prefWidth="10.0" style="-fx-background-color: #2196f3;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <Pane layoutX="195.0" prefHeight="150.0" prefWidth="10.0" style="-fx-background-color: #2196f3;" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <Pane layoutY="140.0" prefHeight="10.0" prefWidth="200.0" style="-fx-background-color: #2196f3;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="140.0" />
      <Pane prefHeight="50.0" prefWidth="300.0" style="-fx-background-color: #e1e1e1;" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
         <children>
            <Button fx:id="saveButton" layoutX="217.0" layoutY="9.0" mnemonicParsing="false" onAction="#saveLists" style="-fx-border-color: #2196f3;" text="Save">
               <font>
                  <Font name="Segoe UI" size="12.0" />
               </font>
            </Button>
         </children>
      </Pane>
      <Label layoutX="20.0" layoutY="91.0" prefWidth="75.0" text="File Name: " AnchorPane.leftAnchor="20.0">
         <font>
            <Font name="Segoe UI" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="fileNamePane" layoutX="95.0" layoutY="85.0" prefHeight="25.0" prefWidth="196.0" promptText="[Enter file name here]" style="-fx-border-color: #2196f3;" AnchorPane.leftAnchor="95.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="85.0">
         <font>
            <Font name="Segoe UI" size="12.0" />
         </font>
      </TextField>
      <Button fx:id="findFileButton" layoutX="14.0" layoutY="40.0" mnemonicParsing="false" onAction="#findFilePath" style="-fx-border-color: #2196f3;" text="File Path" AnchorPane.leftAnchor="14.0" />
      <Label fx:id="pathLabel" layoutX="95.0" layoutY="47.0" prefHeight="20.0" prefWidth="185.0" style="-fx-border-color: #2196f3; -fx-background-color: #e1e1e1;" AnchorPane.leftAnchor="95.0" AnchorPane.rightAnchor="20.0">
         <font>
            <Font name="Segoe UI" size="14.0" />
         </font>
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding>
      </Label>
   </children>
</AnchorPane>
